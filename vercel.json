{
  "version": 2,
  "builds": [
    {
      "src": "mapscraper/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "maxDuration": 300,
        "requirements": "requirements.txt"
      }
    }
  ],
  "env": {
    "DEBUG": "False"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "mapscraper/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}
